  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "build-full": "npm run build-ts && npm run build-ui-prod",
    "serve": "node dist/server.js",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "test-watch": "npm run test -- --watchAll",
    "build-ts": "tsc -p tsconfig.server.json",
    "watch-ts": "tsc -p tsconfig.server.json -w",
    "tslint": "tslint -c tslint.json -p tsconfig.server.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "ng": "ng",
    "start-ui": "ng serve",
    "build-ui": "ng build",
    "build-ui-prod": "ng build --prod --base-href /ui/ --progress false",
    "test-ui": "ng test",
    "lint-ui": "ng lint",
    "e2e-ui": "ng e2e",
    "serve-ui-vue": "vue-cli-service serve",
    "build-ui-vue": "vue-cli-service build",
    "lint-ui-vue": "vue-cli-service lint",
    "test-ui-vue": "vue-cli-service test:unit --config jest.config.ui-vue.js",
    "eject-ui-vue": "vue inspect --mode production | Out-File C:\\Programming\\vue.json",
    "build-schematics": "tsc -p ./schematics/tsconfig.json",
    "generate-module": "node ./node_modules/@angular-devkit/schematics-cli/bin/schematics.js .:module --dry-run false --name",
    "generate-page": "node ./node_modules/@angular-devkit/schematics-cli/bin/schematics.js .:page --dry-run false --styleext scss --name",
    "generate-component": "node ./node_modules/@angular-devkit/schematics-cli/bin/schematics.js .:component --dry-run false --styleext scss --prefix sim --name",
    "heroku-postbuild": "npm run build-full"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "schematics": "./schematics/collection.json",
