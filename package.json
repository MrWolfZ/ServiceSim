  "scripts": {
    "start": "node dist/server.js",
    "build": "npm run build-ts && npm run tslint",
    "build-full": "npm run build-ts && npm run build-ui-prod",
    "serve": "npm run start",
    "test": "jest --forceExit --verbose --detectOpenHandles",
    "test-coverage": "npm run test -- --coverage",
    "test-watch": "npm run test -- --watch",
    "test-watch-coverage": "npm run test-watch -- --coverage",
    "build-ts": "tsc -p tsconfig.server.json",
    "watch-ts": "tsc -p tsconfig.server.json -w",
    "tslint": "tslint -c tslint.json -p tsconfig.server.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "serve-ui": "vue-cli-service serve",
    "build-ui": "vue-cli-service build --mode development",
    "build-ui-prod": "vue-cli-service build",
    "lint-ui": "vue-cli-service lint",
    "test-ui": "vue-cli-service test:unit --config jest.config.ui.js",
    "eject-ui": "vue inspect --mode production | Out-File C:\\Programming\\vue.json",
    "heroku-postbuild": "npm run build-full"
  },
  "engines": {
    "node": ">=8.0.0 <10.0.0",
    "npm": ">=5.0.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "dist/.data/**/*"
    ],
    "delay": 250
  },
  "_moduleAliases": {
    "src": "dist"
  },
