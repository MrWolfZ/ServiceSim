{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Generate Test Page",
      "program": "${workspaceRoot}/node_modules/@angular-devkit/schematics-cli/bin/schematics.js",
      "args": [
        ".:page",
        "--dry-run",
        "false",
        "--styleext",
        "scss",
        "--name",
        "test",
        "--module",
        "test"
      ]
    }
  ]
}
